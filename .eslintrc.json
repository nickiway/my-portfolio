{

  "settings": {
      "import/resolver": {
        "typescript": {}
      }
  },
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "prettier"
  ],
  "rules": {
      "prettier/prettier": ["error"],
      "react/react-in-jsx-scope": "off",
      "react/jsx-uses-react": "off"
  }
}
